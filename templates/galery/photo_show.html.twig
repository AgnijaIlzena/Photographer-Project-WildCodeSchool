{% extends 'base.html.twig' %}

{% block title %}Photo #{{ photo.number }}{% endblock %}


{% block body %}
<div class="grid xs2 sm3 gutter-0">
    <div class="primary flex vcenter fx_center">
        <img class="rounded mx-auto d-block" src="{{ ('/images/' ~ photo.path)|imagine_filter('banner') }}"  alt="{{ photo.number }}">
    </div>
</div>
<a href="{{ path("galery_show", {id: galery.id}) }}">
    Retour à la liste à la galerie
</a>
{% endblock %}
